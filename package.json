{
  "name": "dot-language-server",
  "version": "1.0.5",
  "description": "Language server for the DOT language",
  "author": "Niklas Mollenhauer",
  "license": "MIT",
  "scripts": {
    "test": "tsc --noEmit",
    "clean": "rm -rv -- lib/* || true",
    "compile": "tsc",
    "prepare": "npm run clean && npm run compile"
  },
  "bin": {
    "dot-language-server": "bin/server.js"
  },
  "keywords": [
    "graphviz",
    "dot",
    "language",
    "server",
    "protocol",
    "gv",
    "lsp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nikeee/dot-language-support.git"
  },
  "dependencies": {
    "dot-language-support": "^1.2.6",
    "vscode-languageserver": "^5.1.0",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@types/node": "^10.10.1",
    "@types/yargs": "^12.0.0",
    "typescript": "^3.0.3"
  },
  "engines": {
    "node": ">=7.9"
  }
}
